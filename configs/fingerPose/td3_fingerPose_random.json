{
    "env_name": "MyoFingerPoseRandom",
    "env_config":{},
    "num_trainer_workers": 10,
    "policies_to_train":{
        "policy":true
    },
    "policy_classes":{
        "policy": "td3"
    },
    "policy_configs": {
        "policy": {
            "twin_q": true,
            "policy_delay": 2,
            "smooth_target_policy": true,
            "target_noise": 0.2,
            "target_noise_clip": 0.5,

            "actor_hiddens": [256, 256],
            "actor_hidden_activation": "relu",
            "critic_hiddens": [256, 256],
            "critic_hidden_activation": "relu",
            "n_step": 1,

            "exploration_config": {
                "type": "GaussianNoise",
                "random_timesteps": 10000,
                "stddev": 0.1,
                "initial_scale": 1.0,
                "final_scale": 1.0,
                "scale_timesteps": 1
            },
            "buffer_size": 100000,
            "prioritized_replay": true,
            "critic_lr": 1e-3,
            "actor_lr": 1e-3,
            "target_network_update_freq": 0,
            "tau": 0.002,
            "use_huber": false,
            "huber_threshold": 1.0,
            "l2_reg": 1e-6,
            "grad_clip": null,
            "learning_starts": 1500
        }
    },
    "gamma": 0.995,
    "rollout_fragment_length": 1,
    "train_batch_size": 256,
    "lr": 1e-4,
    "num_gpus": 0,
    "extra_trainer_params": {
        "framework": "torch",
        "seed": 0
    },
    "callbacks": "MyoInfoCallbacks",    
    "trainer_class": "td3",
    "episodes_total": 2e4,
    "logdir": "output",
    "trial_name": "td3_fingerPose_random",
    "checkpoint_freq": 100,
    "restore_checkpoint_path": null
}