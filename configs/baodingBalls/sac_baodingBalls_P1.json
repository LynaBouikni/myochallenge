{
    "env_name": "CustomMyoBaodingBallsP1",
    "env_config": {
        "weighted_reward_keys": {
            "pos_dist_1": 1,
            "pos_dist_2": 1,
            "act_reg": 0,
            "alive": 1
        },
        "goal_time_period": [
            1e100,
            1e100
        ]
    },
    "num_trainer_workers": 19,
    "policies_to_train": {
        "policy": true
    },
    "policy_classes": {
        "policy": "sac"
    },
    "policy_configs": {
        "policy": {
            "Q_model": {
                "fcnet_activation": "relu",
                "fcnet_hiddens": [
                    128,
                    128,
                    128
                ]
            },
            "policy_model": {
                "fcnet_activation": "relu",
                "fcnet_hiddens": [
                    128,
                    128,
                    128
                ]
            },
            "normalize_actions": true,
            "no_done_at_end": false,
            "n_step": 10,
            "timesteps_per_iteration": 10000,
            "prioritized_replay": true,
            "buffer_size": 200000,
            "optimization": {
                "actor_learning_rate": 3e-4,
                "critic_learning_rate": 3e-4,
                "entropy_learning_rate": 3e-4
            },
            "grad_clip": null,
            "learning_starts": 50000,
            "target_network_update_freq": 10,
            "worker_side_prioritization": false,
            "min_iter_time_s": 1
        }
    },
    "agent_policy_dict": {
        "agent": "policy"
    },
    "gamma": 0.995,
    "rollout_fragment_length": 1,
    "train_batch_size": 256,
    "lr": 1e-4,
    "num_gpus": 0,
    "extra_trainer_params": {
        "framework": "torch",
        "seed": 0
    },
    "callbacks": "MyoInfoCallbacks",
    "trainer_class": "sac",
    "episodes_total": 1e7,
    "logdir": "output",
    "trial_name": "sac_baoding_balls_p1_fixed_target",
    "checkpoint_freq": 100,
    "restore_checkpoint_path": null
}